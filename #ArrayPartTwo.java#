            Stack<Integer> potentialIndexes = new Stack<>();
            potentialIndexes.push(0);

            while (!potentialIndexes.isEmpty()) {
                int i = potentialIndexes.pop();

                if (i >= n) return true;

                if (i < 0 || game[i] == 1) continue;

                game[i] = 1;

                potentialIndexes.push(i+1);
                potentialIndexes.push(i-1);BFS            0|
